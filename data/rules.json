{
  "rules": {
    "isTriangle": {
      "premisses": {
        "sides": 3
      },
      "conclusion": "triangle"
    },
    "isRightAngledTriangle": {
      "premisses": {
        "triangle": true,
        "rightAngles": 1
      },
      "conclusion": "right-angled_triangle"
    },
    "isIsoscelesTriangle": {
      "premisses": {
        "triangle": true,
        "identicalSides": 2
      },
      "conclusion": "isosceles_triangle"
    },
    "isRightAngledIsoscelesTriangle": {
      "premisses": {
        "right-angled_triangle": true,
        "identicalSides": 2
      },
      "conclusion": "isosceles_right-angled_triangle"
    },
    "isEquilateralTriangle": {
      "premisses": {
        "triangle": true,
        "identicalSides": 3
      },
      "conclusion": "equilateral"
      },
    "isQuadrilateral": {
      "premisses": {
        "sides": 4
      },
      "conclusion": "quadrilateral"
    },
    "isTrapezium": {
      "premisses": {
        "quadrilateral": true,
        "parallelSides": 2
      },
      "conclusion": "trapezium"
    },
    "isParallelogram": {
      "premisses": {
        "quadrilateral": true,
        "parallelSides": 2,
        "identicalSides": 2
      },
      "conclusion": "parallelogram"
    },
    "isDiamond": {
      "premisses": {
        "quadrilateral": true,
        "parallelSides": 2,
        "identicalSides": 4
      },
      "conclusion": "diamond"
    },
    "isRectangle": {
      "premisses": {
        "quadrilateral": true,
        "parallelSides": 2,
        "rightAngles": 4
      },
      "conclusion": "rectangle"
    },
    "isSquare": {
      "premisses": {
        "quadrilateral": true,
        "identicalSides": 4,
        "rightAngles": 4,
        "parallelSides": 2
      },
      "conclusion": "square"
    },
    "isPentagone": {
      "premisses": {
        "sides": 5
      },
      "conclusion": "pentagone"
    },
    "isEquilateralPentagone": {
      "premisses": {
        "pentagone": true,
        "identicalSides": 5
      },
      "conclusion": "equilateral_pentagone"
    }
  }
}